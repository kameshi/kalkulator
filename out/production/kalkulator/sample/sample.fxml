<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<GridPane alignment="center" hgap="10" maxHeight="800.0" maxWidth="600.0" minHeight="600.0" minWidth="400.0" prefHeight="600.0" prefWidth="400.0" vgap="10" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <VBox maxHeight="800.0" maxWidth="600.0" minHeight="600.0" minWidth="400.0" prefHeight="600.0" prefWidth="400.0">
         <children>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <TextArea fx:id="textArea" prefHeight="100.0" prefWidth="400.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button fx:id="deleteB" alignment="CENTER" mnemonicParsing="false" onAction="#deleteOnA" onMouseClicked="#mouseClicked" prefHeight="100.0" prefWidth="100.0" text="CE">
                    <font>
                       <Font size="40.0" />
                    </font>
                  </Button>
                  <Button fx:id="divisionB" alignment="CENTER" mnemonicParsing="false" onAction="#divisionOnA" onMouseClicked="#mouseClicked" prefHeight="100.0" prefWidth="100.0" text="/">
                      <font>
                        <Font size="40.0" />
                      </font>
                  </Button>
                  <Button fx:id="backspaceB" alignment="CENTER" mnemonicParsing="false" onAction="#backspaceOnA" onMouseClicked="#mouseClicked" prefHeight="100.0" prefWidth="200.0" text="BACKSPACE">
                      <font>
                          <Font size="28.0" />
                      </font>
                  </Button>
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button fx:id="sevenB" mnemonicParsing="false" onAction="#sevenOnA" onMouseClicked="#mouseClicked" prefHeight="100.0" prefWidth="100.0" text="7">
                     <font>
                        <Font size="40.0" />
                     </font>
                  </Button>
                  <Button fx:id="eightB" mnemonicParsing="false" onAction="#eightOnA" onMouseClicked="#mouseClicked" prefHeight="100.0" prefWidth="100.0" text="8">
                     <font>
                        <Font size="40.0" />
                     </font>
                  </Button>
                  <Button fx:id="nineB" mnemonicParsing="false" onAction="#nineOnA" onMouseClicked="#mouseClicked" prefHeight="100.0" prefWidth="100.0" text="9">
                     <font>
                       <Font size="40.0" />
                     </font>
                  </Button>
                  <Button fx:id="multiB" mnemonicParsing="false" onAction="#multiOnA" onMouseClicked="#mouseClicked" prefHeight="100.0" prefWidth="100.0" text="x">
                     <font>
                        <Font size="40.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="400.0">
               <children>
                  <Button fx:id="fourB" mnemonicParsing="false" onAction="#fourOnA" onMouseClicked="#mouseClicked" prefHeight="100.0" prefWidth="100.0" text="4">
                       <font>
                           <Font size="40.0" />
                       </font>
                  </Button>
                  <Button fx:id="fiveB" mnemonicParsing="false" onAction="#fiveOnA" onMouseClicked="#mouseClicked" prefHeight="100.0" prefWidth="100.0" text="5">
               <font>
                   <Font size="40.0" />
               </font>
                  </Button>
                  <Button fx:id="sixB" mnemonicParsing="false" onAction="#sixOnA" onMouseClicked="#mouseClicked" prefHeight="100.0" prefWidth="100.0" text="6">
                    <font>
                        <Font size="40.0" />
                    </font>
                  </Button>
                  <Button fx:id="minusB" mnemonicParsing="false" onAction="#minusOnA" onMouseClicked="#mouseClicked" prefHeight="100.0" prefWidth="100.0" text="-">
                    <font>
                        <Font size="40.0" />
                    </font>
                  </Button>
               </children>
            </HBox>
            <HBox alignment="CENTER" maxWidth="400.0" minWidth="400.0" prefHeight="100.0" prefWidth="400.0">
               <children>
                  <Button fx:id="oneB" mnemonicParsing="false" onAction="#oneOnA" onMouseClicked="#mouseClicked" prefHeight="100.0" prefWidth="100.0" text="1">
                    <font>
                       <Font size="40.0" />
                    </font>
                  </Button>
                  <Button fx:id="twoB" mnemonicParsing="false" onAction="#twoOnA" onMouseClicked="#mouseClicked" prefHeight="100.0" prefWidth="100.0" text="2">
                    <font>
                      <Font size="40.0" />
                    </font>
                  </Button>
                  <Button fx:id="threeB" mnemonicParsing="false" onAction="#threeOnA" onMouseClicked="#mouseClicked" prefHeight="100.0" prefWidth="100.0" text="3">
                     <font>
                         <Font size="40.0" />
                     </font>
                   </Button>
                   <Button fx:id="plusB" mnemonicParsing="false" onAction="#plusOnA" onMouseClicked="#mouseClicked" prefHeight="100.0" prefWidth="100.0" text="+">
                       <font>
                           <Font size="40.0" />
                       </font>
                   </Button>
               </children>
            </HBox>
            <HBox alignment="CENTER" maxWidth="400.0" prefHeight="100.0" prefWidth="400.0">
               <children>
                  <Button fx:id="negativeB" mnemonicParsing="false" onAction="#negativeOnA" onMouseClicked="#mouseClicked" prefHeight="100.0" prefWidth="100.0" text="(+/-)">
                     <font>
                       <Font size="28.0" />
                     </font>
                  </Button>
                  <Button fx:id="zeroB" mnemonicParsing="false" onAction="#zeroOnA" onMouseClicked="#mouseClicked" prefHeight="100.0" prefWidth="100.0" text="0">
                      <font>
                          <Font size="40.0" />
                      </font>
                  </Button>
                  <Button fx:id="pointB" mnemonicParsing="false" onAction="#pointOnA" onMouseClicked="#mouseClicked" prefHeight="100.0" prefWidth="100.0" text=",">
                     <font>
                        <Font size="40.0" />
                     </font>
                  </Button>
                   <Button fx:id="equalB" mnemonicParsing="false" onAction="#equalOnA" prefHeight="100.0" prefWidth="100.0" text="=">
                       <font>
                           <Font size="40.0" />
                       </font>
                   </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</GridPane>
